<%@ page contentType="text/html;charset=UTF-8"%><%@ include file="/WEB-INF/jsp/common.jsp"%><!DOCTYPE html><html><head><title></title><script>	$(document).ready(function() {		$(window.parent.document).find("#nav_activity").addClass("active").siblings().removeClass("active");	});</script>     </head><body><ol class="breadcrumb">  <li style="margin-left: -4px;"><a href="#"><span class="glyphicon glyphicon-home" style="top: 2px;"></span> </a></li>  <li class="active">个人中心</li>  <li class="active">活动管理</li>  <li class="active">活动编辑</li></ol><form:form  id="inputForm" method="post" commandName="activity" class="form-horizontal">   <form:hidden path="id"/>   	<fieldset>		<legend>			<small>${op}</small>		</legend>           <div class="form-group">			<label class="col-sm-2 control-label">* 地域</label>	 			<div class="col-sm-3">            	<form:input path="area" class="form-control required" placeholder="标题"/>            </div>        </div>        <div class="form-group">			<label class="col-sm-2 control-label">* 类型</label>	 			<div class="col-sm-3">            	<form:input path="title" class="form-control required" placeholder="标题"/>            </div>        </div>        <div class="form-group">			<label class="col-sm-2 control-label">* 标题</label>	 			<div class="col-sm-3">            	<form:input path="title" class="form-control required" placeholder="标题"/>            </div>        </div>        		<div class="form-group">			<div class="col-sm-offset-2 col-sm-3">				<button class="btn btn-primary" type="submit"><span class="glyphicon glyphicon-ok-sign" style="top: 2px;"></span> ${op}</button>				<a class="btn btn-default" href="${ctx}/activity/"><span class="glyphicon glyphicon-circle-arrow-left" style="top: 2px;"></span> 返回</a>			</div>		</div>						</fieldset>	</form:form></body></html>